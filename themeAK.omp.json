{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#f1184c",
          "style": "plain",
          "type": "root",
          "template": "ðŸœ² "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "transparent",
          "template": "{{ if .Env.WSL_DISTRO_NAME }}{{ $d := lower .Env.WSL_DISTRO_NAME }}{{ if contains \"ubuntu\" $d }}<#de4714>\uf31b </> {{ else if contains \"arch\"  $d}}<#248de5>\udb82\udcc7 </> {{ else if contains \"debian\" $d }}<#d80f56>\ue77d </> {{ else if contains \"fedora\" $d }}<#56a5db>\ue7d9 </> {{ else if contains \"kali\" $d }}<#087908>\uf327 </> {{ else if contains \"nix\" $d }}<#577bc5>\udb84\udd05 </> {{ else }}<#636463>\uf17c </> {{ end }}{{ end }}"
        },
        {
          "foreground": "#FFBD00",
          "style": "plain",
          "template": "{{ .UserName }}'s ",
          "type": "session"
        },
        {
          "foreground": "#00C6F7",
          "options": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }}/ ",
          "type": "path"
        },
        {
          "foreground": "#F62F2E",
          "options": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#98C379",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> \ued0d {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#ffc30f",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> \ue235 {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} ",
          "type": "python"
        },
        {
          "foreground": "#6fd8e6",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> \ue627 {{ .Full }} ",
          "type": "go"
        },
        {
          "foreground": "#32b8f6",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> \ue64c {{ .Full }} ",
          "type": "dart"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#2e9599",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#ffffff",
          "leading_diamond": "\u250b",
          "options": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFBD00",
          "style": "plain",
          "template": "\u2b9e ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}
