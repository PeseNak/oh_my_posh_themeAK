{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#f1184c",
          "style": "plain",
          "type": "root",
          "template": "üú≤ "
        },
        {
          "foreground": "#FFBD00",
          "style": "plain",
          "template": "{{ .UserName }}'s ",
          "type": "session"
        },
        {
          "foreground": "#00C6F7",
          "options": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }}/ ",
          "type": "path"
        },
        {
          "foreground": "#F62F2E",
          "options": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#98C379",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> Ó¥ç {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#ffc30f",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> Óàµ {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} ",
          "type": "python"
        },
        {
          "foreground": "#6fd8e6",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> Óòß {{ .Full }} ",
          "type": "go"
        },
        {
          "foreground": "#32b8f6",
          "options": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</> Óôå {{ .Full }} ",
          "type": "dart"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#2e9599",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#ffffff",
          "leading_diamond": "\u250b",
          "options": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFBD00",
          "style": "plain",
          "template": "\u2b9e ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 4
}
